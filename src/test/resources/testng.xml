<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="run tests in parallel" parallel="tests" verbose="1" configfailurepolicy="continue" thread-count="4"
       data-provider-thread-count="4">
    <listeners>
        <listener class-name="com.nglebanov.selatstar.testng.Listener"></listener>
    </listeners>
    <test name="Tests">
        <classes>
            <class name="com.nglebanov.selatstar.runner.CucumberSelenideRunner"/>
        </classes>
    </test>
</suite>
